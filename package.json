{
	"name": "smartpaci-controlpanel-concept",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"protoc": "run-script-os",
		"protoc:default": "protoc --plugin ./node_modules/.bin/protoc-gen-es --es_out static/generated --es_opt target=ts ./bluetooth.proto",
		"protoc:win32": "protoc --plugin ./node_modules/.bin/protoc-gen-es.cmd --es_out static/generated --es_opt target=ts ./bluetooth.proto",
		"postinstall": "npm run protoc"
	},
	"devDependencies": {
		"@bufbuild/protoc-gen-es": "^1.7.0",
		"@smithy/util-hex-encoding": "^2.1.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.17",
		"cborg": "^4.0.8",
		"class-variance-authority": "^0.7.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"protoc": "^1.1.3",
		"run-script-os": "^1.1.6",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module"
}
